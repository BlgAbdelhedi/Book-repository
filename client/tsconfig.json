{
    "compilerOptions": {
      "target": "ESNext",
      "useUnknownInCatchVariables": false,
      "lib": ["DOM", "ESNext"],
      "module": "ESNext",
      "strict": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "jsx": "react"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  